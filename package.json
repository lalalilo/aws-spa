{
  "name": "aws-spa",
  "version": "3.0.0",
  "description": "A no-brainer script to deploy a single page app on AWS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicgirault/aws-spa.git"
  },
  "author": "nicgirault <nic.girault@gmail.com>",
  "keywords": [
    "react",
    "vue",
    "angular",
    "create-react-app",
    "deploy",
    "aws",
    "s3",
    "cloudfront",
    "acm",
    "route53"
  ],
  "tags": [
    "react",
    "deploy",
    "aws"
  ],
  "bugs": {
    "url": "https://github.com/nicgirault/aws-spa/issues"
  },
  "homepage": "https://github.com/nicgirault/aws-spa#readme",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d lib --extensions '.ts'",
    "prebuild": "yarn check-types && yarn test",
    "run-cli": "babel-node --extensions '.ts' src/cli.ts",
    "check-types": "tsc",
    "test": "jest"
  },
  "bin": {
    "aws-spa": "./lib/cli.js"
  },
  "files": [
    "lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-types"
    }
  },
  "release": {
    "branches": [
      "master",
      "next"
    ]
  },
  "dependencies": {
    "adm-zip": "^0.5.9",
    "aws-sdk": "^2.1215.0",
    "inquirer": "^7.0.0",
    "mime-types": "^2.1.35",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/node": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.22",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.7",
    "@types/rimraf": "^3.0.2",
    "@types/yargs": "^17.0.12",
    "codecov": "^3.8.3",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "typescript": "^3.7.2"
  }
}
